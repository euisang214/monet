{
  "name": "monet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx scripts/checkEnv.ts && NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "tsx scripts/checkEnv.ts && next start",
    "lint": "next lint",
    "dev:fresh": "npm run setup-mock && npm run dev",
    "setup-mock": "NODE_ENV=development tsx scripts/mockData.ts",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "stripe:trigger": "stripe trigger payment_intent.succeeded",
    "dev:with-stripe": "concurrently \"npm run dev\" \"npm run stripe:listen\""
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^2.4.2",
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.0.0",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "next": "15.3.3",
    "next-auth": "^4.24.0",
    "react": "^19.0.0",
    "react-big-calendar": "^1.19.3",
    "react-dom": "^19.0.0",
    "stripe": "^14.0.0",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
